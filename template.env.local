# Copy this file to .env.local and fill the required variables
# You need to create a WalletConnect project at https://cloud.walletconnect.com/
# Variables are PREFIXED with NEXT_PUBLIC_ so that they are available client side

# Project name (mandatory)
NEXT_PUBLIC_PROJECT_NAME=

# Project ID (mandatory)
NEXT_PUBLIC_PROJECT_ID=

# Project description (optional)
NEXT_PUBLIC_PROJECT_DESCRIPTION=
